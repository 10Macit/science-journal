    <string name="sensor_desc_short_decibel" msgid="6022620562641402361">"Jakost zvoka, ki doseže tipalo ali mikrofon, merjena v decibelih (dB)."</string>
    <string name="sensor_desc_short_light" msgid="2836912567230826095">"Količina svetlobe, ki doseže tipalo svetlobe v okolju, merjena v luksih."</string>
    <string name="sensor_desc_short_magnetic_rotation" msgid="2856177797841108478">"Kot vrtenja med telefonom in zunanjim magnetnim poljem."</string>
    <string name="sensor_desc_short_bluetooth" msgid="6408146877850831242">"Zunanje tipalo prikazuje podatke tukaj glede na nastavitve po meri."</string>
