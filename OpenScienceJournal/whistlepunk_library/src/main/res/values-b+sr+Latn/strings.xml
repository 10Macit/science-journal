    <string name="sensor_desc_short_magnetic_rotation" msgid="2856177797841108478">"Ugao rotacije između telefona i spoljnog magnetnog polja."</string>
    <string name="sensor_desc_short_bluetooth" msgid="6408146877850831242">"Ovde se prikazuju podaci sa spoljnog senzora, na osnovu prilagođenih podešavanja."</string>
