    <string name="sensor_desc_short_light" msgid="2836912567230826095">"Množstvo svetla merané v luxoch, ktoré dopadá na senzor okolitého svetla."</string>
    <string name="sensor_desc_short_magnetic_rotation" msgid="2856177797841108478">"Uhol natočenia medzi telefónom a vonkajším magnetickým poľom."</string>
    <string name="sensor_desc_short_bluetooth" msgid="6408146877850831242">"V tejto časti sa zobrazujú údaje externého senzora na základe vlastných nastavení."</string>
